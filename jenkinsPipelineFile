pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                sh 'echo "=======build stage ======="'
            }
        }

        stage('test') {
            steps {
                sh '======= test stage ======='
            }
        }

        stage('deploy') {
            steps {
                sh '======= deploy stage ======='
            }
        }


    }
}
